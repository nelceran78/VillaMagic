<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

@if(TempData["exitoso"] != null)
{
    <script>
        Swal.fire({
            position: 'center-center',
            icon: 'success',
            title: '@TempData["exitoso"]',
            showConfirmButton: false,
            timer: 3000
        })
    </script>
}

@if (TempData["error"] != null)
{
    <script>
        Swal.fire({
            position: 'center-center',
            icon: 'error',
            title: '@TempData["error"]',
            showConfirmButton: false,
            timer: 3000
        })
    </script>
}